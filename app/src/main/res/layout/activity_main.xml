<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_saveStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="保存字符串" />

    <Button
        android:id="@+id/btn_getStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="获取字符串" />

    <Button
        android:id="@+id/btn_saveJSONObject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="保存JSONObject" />

    <Button
        android:id="@+id/btn_getJSONObject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="获取JSONObject" />

    <Button
        android:id="@+id/btn_saveObject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="保存Object"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_getObject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="获取Object"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_saveBitmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="保存Bitmap"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_getBitmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="获取Bitmap"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_saveDrawable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="保存Drawable"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_getDrawable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="获取Drawable"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="关闭Close"
        android:textAllCaps="false" />

    <TextView
        android:id="@+id/tv_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>